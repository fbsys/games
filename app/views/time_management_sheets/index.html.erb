<h1>Listing time_management_sheets</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Year</th>
      <th>Month</th>
      <th>Employee num</th>
      <th>Company name</th>
      <th>Department code</th>
      <th>Instruction id</th>
      <th>Team code</th>
      <th>Task id</th>
      <th>Process id</th>
      <th>Man hour</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_management_sheets.each do |time_management_sheet| %>
      <tr>
        <td><%= time_management_sheet.id %></td>
        <td><%= time_management_sheet.year %></td>
        <td><%= time_management_sheet.month %></td>
        <td><%= time_management_sheet.employee_num %></td>
        <td><%= time_management_sheet.company_name %></td>
        <td><%= time_management_sheet.department_code %></td>
        <td><%= time_management_sheet.instruction_id %></td>
        <td><%= time_management_sheet.team_code %></td>
        <td><%= time_management_sheet.work_task_id %></td>
        <td><%= time_management_sheet.work_process_id %></td>
        <td><%= time_management_sheet.man_hour %></td>
        <td><%= link_to 'Show', time_management_sheet %></td>
        <td><%= link_to 'Edit', edit_time_management_sheet_path(time_management_sheet) %></td>
        <td><%= link_to 'Destroy', time_management_sheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time management sheet', new_time_management_sheet_path %>
